server {
    #listen       80;
    server_name  192.168.10.100;

    root   /vagrant/www/web;
    index  index.php;

    location ~ \.php$ {
      fastcgi_pass   unix:/var/run/php-fpm-www.sock;
      fastcgi_index  index.php;
      fastcgi_param  SCRIPT_FILENAME  $document_root$fastcgi_script_name;
      include        fastcgi_params;
    }
}
